		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.6.8",
		"bootstrap": "^5.3.3",
		"buffer": "^6.0.3",
		"moment": "^2.30.1",
		"node-localstorage": "^3.0.5",
		"svelte-feather-icons": "^4.1.0",
		"sweetalert2": "^11.10.8",
		"toastify-js": "^1.12.0",
		"yup": "^1.4.0"
	}
}
